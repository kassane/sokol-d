		{
			"name": "blend",
			"sourceFiles": [
				"blend.d",
				"shaders/blend.d"
			],
			"targetPath": "build",
			"targetType": "executable",
			"dependencies": {
				"sokol-d": {
					"path": ".."
				}
			},
			"environments": {
				"SOKOL_ROOTPATH": "${SOKOL_D_PACKAGE_DIR}"
			},
			"configurations": [
				{
					"name": "native",
					"subConfigurations": {
						"sokol-d": "sokol-static"
					}
				},
				{
					"name": "wasm",
					"targetType": "staticLibrary",
					"postGenerateCommands": [
						"$DC -run ${SOKOL_D_PACKAGE_DIR}/build.d -- --target=wasm --backend=gles3 --link=blend --verbose"
					],
					"targetName": "blend",
					"subConfigurations": {
						"sokol-d": "sokol-wgles3"
					}
				},
				{
					"name": "wgpu",
					"targetType": "staticLibrary",
					"postGenerateCommands": [
						"$DC -run ${SOKOL_D_PACKAGE_DIR}/build.d -- --target=wasm --backend=wgpu --link=blend --verbose"
					],
					"targetName": "blend",
					"subConfigurations": {
						"sokol-d": "sokol-wgpu"
					}
				}
			]
		},
