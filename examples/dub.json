	"buildTypes": {
		"debug": {
			"buildOptions": [
				"debugMode",
				"debugInfo"
			]
		},
		"release": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"optimize"
			]
		},
		"release-nobounds": {
			"buildOptions": [
				"releaseMode",
				"inline",
				"noBoundsCheck",
				"optimize"
			]
		},
		"release-betterc": {
			"buildOptions": [
				"betterC",
				"releaseMode",
